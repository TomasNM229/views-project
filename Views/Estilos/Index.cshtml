@model IEnumerable<views_project.Models.Estilos> // Necesitamos que corra iterativamente todas las instancias


<p>
    <a asp-action="Create">Nuevo</a>
</p>
<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
        </tr>
    </thead>
    <tbody>
        @*Aqui entr√≥*@
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IDEspecialidad">Editar</a>
                </td>
                <td>
                    <a asp-action="Delete" asp-route-id="@item.IDEspecialidad">Eliminar</a>
                </td>
            </tr>
        }
    </tbody>
</table>